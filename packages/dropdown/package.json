{
    "name": "@posten-hedwig/dropdown",
    "version": "1.0.6",
    "publishConfig": {
        "access": "public"
    },
    "description": "Hedwig styled dropdowns",
    "keywords": [
        "Hedwig",
        "Dropdown"
    ],
    "author": "Helle Hiorth-Sch√∏yen <helle@solidsquare.no>",
    "homepage": "https://github.com/bring/hedwig-react-components#readme",
    "license": "ISC",
    "source": "src/index.js",
    "main": "dist/index.js",
    "module": "dist/module.js",
    "files": [
        "dist"
    ],
    "scripts": {
        "prebuild": "rimraf dist && npm run sass",
        "sass": "sass sass/dropdown.scss:dist/dropdown.scss --style compressed --no-source-map",
        "build": "babel src -d dist --root-mode=upward --ignore '**/*.stories.js'",
        "build:parcel": "npm run prebuild && parcel build"
    },
    "dependencies": {
        "@posten-hedwig/core": "^0.0.9",
        "@posten-hedwig/label": "^1.0.3",
        "prop-types": "^15.8.1"
    },
    "peerDependencies": {
        "react": ">=16.14.x"
    }
}
