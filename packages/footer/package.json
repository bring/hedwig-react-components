{
    "name": "@posten-hedwig/footer",
    "version": "1.0.0",
    "publishConfig": {
        "access": "public"
    },
    "description": "Provides footer in Posten and Bring style",
    "keywords": [
        "footer",
        "navigation"
    ],
    "author": "Viggo Normann <v@iggo.no>",
    "homepage": "https://github.com/bring/hedwig-react-components#readme",
    "license": "ISC",
    "main": "dist/index.js",
    "files": [
        "dist"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/bring/hedwig-react-components.git"
    },
    "scripts": {
        "prebuild": "rimraf dist && npm run sass",
        "sass": "sass sass/footer.scss:dist/footer.scss --style compressed --no-source-map",
        "build": "babel src -d dist --root-mode=upward --ignore '**/*.stories.js'"
    },
    "bugs": {
        "url": "https://github.com/bring/hedwig-react-components/issues"
    },
    "dependencies": {
        "@fortawesome/free-brands-svg-icons": "^6.1.1",
        "@fortawesome/free-solid-svg-icons": "^6.1.1",
        "@fortawesome/react-fontawesome": "^0.1.18",
        "@posten-hedwig/accordion": "^1.0.8",
        "@posten-hedwig/block": "^1.0.7",
        "@posten-hedwig/button": "^1.0.7",
        "@posten-hedwig/container": "^1.0.0",
        "@posten-hedwig/core": "^0.0.8",
        "@posten-hedwig/grid": "^1.0.0",
        "@posten-hedwig/linklist": "^1.0.7",
        "@posten-hedwig/logo": "^1.0.0"
    },
    "devDependencies": {
